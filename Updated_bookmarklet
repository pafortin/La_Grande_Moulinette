javascript:(async function() {var url=window.location.href;var selector="div.t-14.flex.align-items-center";var elements=document.querySelectorAll(selector);var results=[];for(var i=0;i<elements.length;i++){results.push(elements[i].innerText);}var data={url:url,content:results};try{await navigator.clipboard.writeText(JSON.stringify(data));console.log("Résultat copié dans le presse-papiers!");}catch(err){console.error("Erreur lors de la copie dans le presse-papiers. Essayez de cliquer sur la page et de relancer le bookmarklet.");}})();
